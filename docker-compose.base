version: '2'
services:
    endvote:
        container_name: endvote
        image: registry.gitlab.com/brombom43531/letvote-backend:REPLACE_GIT_LAST_HASH       
        restart: always
        ports:
            - 3000:3000
        environment:
            - NODE_ENV=production
            - URL=https://letvote.wtf
            - DATABASE=127.0.0.8
        networks:
            ubuntu_default:
                ipv4_address: 172.20.0.4
networks:
    ubuntu_default:
        external: true
